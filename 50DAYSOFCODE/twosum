import java.util.Scanner;
public class twosum{
    public static void main (String args[])
    {
        int a[]={13,6,9,2,4};
        int n=a.length;
        int target=11;
        for (int i = 0; i < n; i++) {
            for (int j = i + 1; j < n; j++) {
                if (a[i] + a[j] == target) {
                    System.out.println("Elements are " + a[i] + " and " + a[j]);
                }
            }
        }
    }
}
