import java.util.Scanner;
public class fibanocci {
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        System.out.print("enter the number");
        int n=sc.nextInt();
        int a=0,b=1;
        int c=0;
        for(int i=1;i<=n;i++)
        {
            System.out.print(a+",");
            c=a+b;
            a=b;
            b=c;
        }
    }
}
