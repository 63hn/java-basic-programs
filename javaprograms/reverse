import java.util.Scanner;
public class reverse {
    public class void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        System.out.print("enter the number");
        int num=sc.nextInt();
        int temp=0,rev=0;
        while(num<0)
        {
            temp=num%10;
            rev=rev*10+temp;
            num=num/10;
        }
        System.out.print(rev);
    }
}
